---

import Footer from "components/Footer.astro";
import Intro from "components/Intro.astro";
import Nav from "components/Nav.astro";
import Rates from "components/Rates.astro";
import Subscription from "components/Subscription.astro";
import Viewed from "components/Viewed.astro";
import PageLayout from "layouts/PageLayout.astro";
import Converter from "components/Converter.astro";

import { getAllRates } from "../services/rates";
const rates = await getAllRates();
---

<PageLayout>
  <Nav />
  <Intro />
  <Rates rates={rates} />
  <Viewed />
  <Converter rates={rates}/>
  <Subscription />
  <Footer />
</PageLayout>
